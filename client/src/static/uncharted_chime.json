{
  "metadata": {
    "source": "modelRepo/easel/sir.easel",
    "type": "easel",
    "version": "1.0",
    "knowledge": "https://code-for-philly.gitbook.io/chime/",
    "name": "CHIME SIR Model",
    "created": "2020-03-15",
    "description": "The model consists of individuals who are either Susceptible (S), Infected (I), or Recovered (R). etc."
  },
  "nodes": [
    {
      "id": "1",
      "concept": "Susceptible",
      "label": "Susceptible",
      "type": "variable",
      "metadata": {
        "name": "Susceptible",
        "description": "Population of people who are susceptible to be infected",
        "expression": "-1.0 * (beta * (1 - contact_rate) * Susceptible * Infected / total_pop)",
        "units": "number of people",
        "knowledge": ""
      }
    },
    {
      "id": "2",
      "concept": "Infected",
      "label": "Infected",
      "type": "variable",
      "metadata": {
        "name": "Infected",
        "description": "Population of people who are infected",
        "expression": "(beta * (1 - contact_rate) * Susceptible * Infected / total_pop) + -1.0 * (gamma * Infected)",
        "units": "number of people",
        "knowledge": ""
      }
    },
    {
      "id": "3",
      "concept": "Recovered",
      "label": "Recovered",
      "type": "variable",
      "metadata": {
        "name": "Recovered",
        "description": "Population of people who recovered (and have immunity now)",
        "expression": "gamma * Infected",
        "units": "number of people",
        "knowledge": ""
      }
    },
    {
      "id": "4",
      "concept": "policy_days",
      "label": "policy_days",
      "type": "constant",
      "metadata": {
        "name": "policy_days",
        "description": "",
        "expression": "",
        "units": "",
        "knowledge": ""
      }
    },
    {
      "id": "5",
      "concept": "betas",
      "label": "betas",
      "type": "constant",
      "metadata": {
        "name": "betas",
        "description": "",
        "expression": "",
        "units": "",
        "knowledge": ""
      }
    },
    {
      "id": "6",
      "concept": "contact_rate",
      "label": "contact_rate",
      "type": "constant",
      "metadata": {
        "name": "Contact Rate",
        "description": "The social contact rate.",
        "expression": "",
        "units": "",
        "knowledge": ""
      }
    },
    {
      "id": "7",
      "concept": "total_pop",
      "label": "total_pop",
      "type": "variable",
      "metadata": {
        "name": "Total Population",
        "description": "The total size of the population.",
        "expression": "Susceptible + Infected + Recovered",
        "units": "number of people",
        "knowledge": ""
      }
    },
    {
      "id": "8",
      "concept": "beta",
      "label": "beta",
      "type": "variable",
      "metadata": {
        "name": "Effective Contact Rate",
        "description": "The probability of transmitting disease between a susceptible and an infectious individual.",
        "expression": "(AIR_time <= policy_days[0])*betas[0] + (AIR_time > policy_days[0] and AIR_time <= policy_days[1])*betas[1] + (AIR_time > policy_days[1])*betas[2] + gamma",
        "units": "",
        "knowledge": ""
      }
    },
    {
      "id": "9",
      "concept": "gamma",
      "label": "gamma",
      "type": "constant",
      "metadata": {
        "name": "Inverse of the Mean Recovery Time",
        "description": "The recovery rate, 1/D, determined by the average duration, D, of infection.",
        "expression": "1.0/14.0",
        "units": "",
        "knowledge": ""
      }
    }
  ],
